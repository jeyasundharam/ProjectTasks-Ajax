<!DOCTYPE html>
<html>
  <head>
    <title>Projecttasks</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  
  </head>


  <body>
   <center>
   <h1>Project Shedule</h1>
    <%= link_to "New",:controller => "projects",:action=>"insert" %>
    <%= link_to "Show",:controller => "projects",:action=>"show" %>
    <%= link_to "Multi tasks",:controller => "projects",:action=>"multitasks" %>
    <%= link_to "Show All tasks",:controller => "projects",:action=>"showtasks" %>
    <br>
    <% if flash[:notice] %>
      <%= flash[:notice] %>
    <% end %>
    <%= yield %>
  </body>
</html>
